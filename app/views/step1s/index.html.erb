<h3>Welcome to BizCentrik Source 2 Target mapping wizard</h3>

<table>
  <tr>
    <th>Project name</th>
    <th>Phase name</th>
    <th>Project urls</th>
    <th>Subject area</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @step1s.each do |step1| %>
  <tr>
    <td><%= step1.project_name %></td>
    <td><%= step1.phase_name %></td>
    <td><%= step1.project_urls %></td>
    <td><%= step1.subject_area %></td>
    <td><%= link_to 'Show', step1 %></td>
    <td><%= link_to 'Edit', edit_step1_path(step1) %></td>
    <td><%= link_to 'Destroy', step1, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Proceed Next', new_step1_path %>
